{
  "name": "web-sql-batch-helper",
  "version": "0.1.0-pre3",
  "description": "Web SQL transaction helper if not all SQL is available in the same tick",
  "main": "dist/index.cjs.js",
  "jsnext:main": "dist/index.es6.js",
  "scripts": {
    "build": "coffee -c index.coffee",
    "prepublish": "rm -rf dist && mkdir dist && coffee -p index.coffee > dist/index.es6.js && rollup dist/index.es6.js -o dist/index.cjs.js -f cjs",
    "start": "rollup testIndex.js -o testBundle.js -f cjs",
    "start2": "rollup cordovaTestIndex.js -o cordovaTestApp/www/testBundle.js -f cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web",
    "sql",
    "batch"
  ],
  "author": "Christopher J. Brody",
  "license": "ISC OR MIT",
  "devDependencies": {
    "coffee-script": "^1.11.1",
    "rollup": "^0.36.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brodybits/web-sql-batch-helper.git"
  },
  "bugs": {
    "url": "https://github.com/brodybits/web-sql-batch-helper/issues"
  },
  "homepage": "https://github.com/brodybits/web-sql-batch-helper#readme"
}
